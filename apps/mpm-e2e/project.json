{
  "name": "mpm-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/mpm-e2e/src",
  "implicitDependencies": [
    "mpm"
  ],
  "targets": {
    "e2e": {
      "executor": "@nx/playwright:test",
      "options": {
        "headless": true,
        "watchForFileChanges": false,
        "outputDir": "test-results/apps/mpm-e2e/playwright"
      }
    },
    "e2e-ci": {
      "executor": "@nx/playwright:test",
      "options": {
        "headless": true,
        "watchForFileChanges": false,
        "outputDir": "test-results/apps/mpm-e2e/playwright",
        "ci": true
      }
    }
  }
}