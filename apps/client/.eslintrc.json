{
  "extends": [
    "plugin:@nx/react",
    "../../.eslintrc.json",
    "prettier"
  ],
  "ignorePatterns": [
    "!**/*"
  ],
  "rules": {
    "max-len": [
      "error",
      {
        "code": 80
      }
    ],
    "prettier/prettier": "error"
  },
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.tsx",
        "*.js",
        "*.jsx"
      ],
      "rules": {}
    },
    {
      "files": [
        "*.ts",
        "*.tsx"
      ],
      "rules": {}
    },
    {
      "files": [
        "*.js",
        "*.jsx"
      ],
      "rules": {}
    }
  ]
}